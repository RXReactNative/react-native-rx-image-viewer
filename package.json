{
  "name": "react-native-image-pan-zoom",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "clean": "rimraf app/package/dist",
    "lib-build": "npm run clean && babel app/package/src -d app/package/dist --source-maps",
    "clearFolder": "node ./script/web/rm-docs.js",
    "web-build": "set NODE_ENV=production && npm run clearFolder && webpack -p --config ./web/webpack.prod.config.js",
    "web-dev": "set NODE_ENV=development && npm run clearFolder && webpack-dev-server -d --config ./web/webpack.dev.config.js --port 3001 --inline --hot --colors  --history-api-fallback",
    "web-devbuild": "set NODE_ENV=development && npm run clearFolder && webpack -p --config ./web/webpack.dev.config.js"
  },
  "dependencies": {
    "react": "16.8.3",
    "react-art": "16.6.3",
    "react-dom": "16.8.3",
    "react-native": "^0.63.0",
    "react-native-safe-area-context": "^3.0.7",
    "react-native-web": "^0.11.0-alpha.3",
    "react-native-screens": "^2.9.0",
    "react-native-reanimated": "^1.9.0",
    "react-navigation": "^4.4.0",
    "react-navigation-stack": "^2.8.2",
    "react-navigation-tabs": "^2.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/cameraroll": "^4.0.0",
    "babel-cli": "^6.26.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "7.1.5",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-react-native-web": "^0.11.5",
    "babel-plugin-react-transform": "3.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-native": "4.0.1",
    "babel-preset-stage-0": "6.24.1",
    "css-loader": "^3.1.0",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "prettier": "^1.15.3",
    "react-native-gesture-handler": "1.4.1",
    "react-native-md5": "^1.0.0",
    "react-native-web-image-loader": "0.1.1",
    "react-test-renderer": "16.8.6",
    "rimraf": "^3.0.2",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
